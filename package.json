{
  "name": "floof",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:dev": "nodemon --exec babel-node app.js",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo:all",
    "seed": "sequelize-cli db:seed:all",
    "run:db": "npm run migrate:undo && npm run migrate",
    "remove:build": "rm -rf ./build",
    "build": "npm run remove:build && babel app.js -d build && babel server -d build/server",
    "start": "node build/app.js",
    "heroku-postbuild": "npm run build && npm run run:db && npm run seed"
  },
  "author": "zokky",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.15.3",
    "app-root-path": "^3.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "redis": "^3.1.2",
    "sequelize": "^6.6.5",
    "sequelize-cli": "^6.2.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/node": "^7.14.9",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.1",
    "nodemon": "^2.0.12"
  }
}
